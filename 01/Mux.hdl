/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=o_notsel);
    And(a=a, b=o_notsel, out=o_and_anotsel);
    And(a=b, b=sel, out=o_and_bsel);
    Or(a=o_and_anotsel, b=o_and_bsel, out=out);
}