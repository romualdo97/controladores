/**
 * Adds two 16-bit values.
 * The most significant carry bit is ignored.
 */

CHIP Equal {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na);
    Not(in=b, out=nb);

    And(a=a, b=b, out=abo);
    And(a=na, b=nb, out=nanbo);

    Or(a=abo, b=nanbo, out=out);
}